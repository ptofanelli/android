<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ifspsaocarlos.sdm.mensageirows">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mensageiro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_mensageiro"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.EnviarActivity" />
        <activity android:name=".view.LerActivity" />
        <activity android:name=".view.MostrarMensagemActivity" />
        <activity android:name=".view.MostrarMensagensActivity" />
        <activity android:name=".view.NovoContatoActivity"></activity>
    </application>

</manifest>